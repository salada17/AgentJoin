@model SugarHill.OfficeSquare.Component.BindingModels.Support.SendEmailBindingModel
@inject ValidationHtmlAttributeProvider validationHtmlAttributeProvider
@inject ICurrentUserAccessor<ApplicationUser> currentUserAccessor
@inject JsonSerializerSettings _jsonSerializerSettings

@{
    ViewData["Title"] = "자주묻는질문";
    Layout = "~/Views/Shared/_Base.cshtml";

}

@section pagecss {
    <environment include="Development">
        <link asp-append-version="true" rel="stylesheet" href="~/css/gen2/web/agentJoin.css" />
    </environment>
    <environment exclude="Development">
        <link asp-append-version="true" rel="stylesheet" href="~/css/gen2/web/agentJoin.min.css" />
    </environment>
}

<div id="AgentJoinContainerPlaceholder"></div>

@section scripts {
    <script>
        ReactDOM.render(
            React.createElement(
                WebAgentJoinContainer,
                {
                    isAuthenticated: isAuthenticated,
                    currentUserFullName: currentUserFullName,
                    requestPath: requestPath,
                    currentController: currentController,
                    currentAction: currentAction,
                    renderType: 3
                }),
            document.getElementById("AgentJoinContainerPlaceholder")
        );
    </script>
}